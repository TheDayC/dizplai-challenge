{
    "name": "dizplai-challenge",
    "version": "1.0.0",
    "description": "A full stack voting application for Dizplai's developer challenge.",
    "main": "index.js",
    "scripts": {
        "prepare": "husky",
        "lint:staged:api": "npm --prefix ./api/ run lint:staged",
        "lint:staged:ui": "npm --prefix ./ui/ run lint:staged",
        "knip:ui": "npm --prefix ./ui/ run knip",
        "knip:api": "npm --prefix ./api/ run knip",
        "test:ui": "npm --prefix ./ui/ run test",
        "test:api": "npm --prefix ./api/ run test",
        "test": "echo \"Error: no test specified\" && exit 1",
        "docker": "docker compose up -d"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/TheDayC/dizplai-challenge.git"
    },
    "author": "David Connolly",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/TheDayC/dizplai-challenge/issues"
    },
    "homepage": "https://github.com/TheDayC/dizplai-challenge#readme",
    "devDependencies": {
        "@commitlint/cli": "^19.7.1",
        "@commitlint/config-conventional": "^19.7.1",
        "husky": "^9.1.7"
    },
    "volta": {
        "node": "22.13.1",
        "npm": "11.1.0"
    }
}
